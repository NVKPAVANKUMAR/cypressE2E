    "mochawesome": "4.1.0",
    "mochawesome-merge": "2.0.1",
    "mochawesome-report-generator": "4.1.0"
  },
  "dependencies": {
    "del": "^5.1.0",
    "rimraf": "^3.0.0"
  }
